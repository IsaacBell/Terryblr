- rep        = @reports[rep_title.to_sym]
- data       = rep[:results]
- dimensions = rep[:dimensions]
- metrics    = rep[:metrics]
- range      = (rep[:start_date].to_date .. rep[:end_date].to_date).to_a
%tbody{:units => 'visit'}
  - range.each do |date|
    / Terryblr::Order visitors by date
    - point = data.points.detect{|p|p.dimensions.first.values.first.to_i == date.mday}
    - next unless point
    %tr
      %td= date.to_s(:db)
      %td= point.metrics.map{|m| m.values }.flatten.join(' ')