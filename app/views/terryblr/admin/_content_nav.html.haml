#content_nav
  %ul
    - %w(post photos video).each do |content_type|
      %li{:class => content_type}= link_to ttt('terryblr.' << content_type.to_s), admin_new_content_path(content_type)
    %li.features= link_to ttt(:'terryblr.features'), admin_features_path
    %li.archives= link_to ttt(:'terryblr.archives'), admin_posts_path
    %li.pages= link_to ttt(:'terryblr.pages'), admin_pages_path

